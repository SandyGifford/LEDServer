		"hsl-to-rgb": "^1.0.0",
		"html-webpack-plugin": "^5.3.2",
		"mini-css-extract-plugin": "^2.4.2",
		"pug": "^3.0.2",
		"pug3-loader": "^2.4.3",
		"react": "^17.0.2",
		"react-colorful": "^5.5.0",
		"react-dom": "^17.0.2",
		"react-error-boundary": "^3.1.3",
		"reset-css": "^5.0.1",
		"sass": "^1.42.1",
