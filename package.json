{
	"name": "led-server",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"makePyEnv": "python3 -m venv .venv",
		"activatePyEnvp--doesntWork": ". ./.venv/bin/activate",
		"pyInstall": "./.venv/bin/pip3 install -r ./pyReqs.txt",
		"pyLocalInstall": "./.venv/bin/pip3 install -r ./pyReqs.txt",
		"updatePyReqs": "npm run updateAllPyReqs && npm run updateLocalPyReqs",
		"updateAllPyReqs": "./.venv/bin/pip3 freeze > ./pyReqs.txt",
		"updateLocalPyReqs": "./.venv/bin/pip3 freeze | grep -iv adafruit > ./pyLocalReqs.txt",

		"dev": "webpack --watch --progress",
		"server": "sudo ./.venv/bin/python3 dist/server/app.py",

		"sync": "rsync -a --exclude .env --exclude dist/server/files/__pycache__ --exclude node_modules --exclude .venv . pi@ledserver:/home/pi/LEDServer"
	},
	"author": "",
	"license": "ISC",
